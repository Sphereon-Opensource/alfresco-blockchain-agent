{
  "request": {
    "urlPath": "/v2",
    "method": "POST",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "method",
          "equalTo": "reveal-entry"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "params.entry",
          "equalTo": "dummy-hash-002-reveal-entry"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "jsonrpc",
          "equalTo": "2.0"
        }
      }
    ]
  },
  "response": {
    "status": 400
  }
}
